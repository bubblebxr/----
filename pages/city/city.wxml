<!--搜索框  -->
<navigator url="../search/search">
  <view class="header">
    <view class="search">搜索</view>
  </view>
</navigator>
<!-- 选择城市栏 -->
<block wx:for="{{datalist}}" wx:key="index">
  <view bindtap="goDetail" data-id="{{item}}" class="city">
    <text class="text">{{item.city}}</text>
    <image src="https://6278-bxr-9gc8orxf1bdb5402-1320348595.tcb.qcloud.la/next.png" class="img"></image>
  </view>
</block>