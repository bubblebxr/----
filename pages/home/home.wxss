/* 轮播图 */
.swip{
  margin-top:30rpx;
  margin-left:10%;
  margin-right:10%;
  overflow: hidden;
  margin-bottom:30rpx;
  border-radius: 30rpx;
}
/* 选择城市 */
.tip{
  text-align: center;
  margin-bottom:20rpx;
  color:rgb(92, 86, 86);
  font-size:28rpx;
}
.btn{
  font-size: 24rpx;
  padding: 12rpx 19rpx;
  border: 1px solid #dcdcdc;
  background: #F8F8F8;
  border-radius: 10rpx;
  margin-left:36rpx;
  margin-right: 20rpx;
  margin-bottom: 22rpx;
  text-align: center;
  display: inline-block;
  color: #666666;
}
.btn:nth-child(35) {
margin-right: 0;
}
.btn-selected{
  border: 1px solid goldenrod;
  color: goldenrod;
  background: #FFFFFF;
  font-weight:bold;
}
/* 足迹地图按钮 */
.enlighten button{
  margin-top:20rpx;
  margin-bottom:20rpx;
  padding-left:40rpx;
  font-weight:normal;
  border:2rpx solid black;
  background-color:white;
  color:black;
}
/* 旅行记录 */
.all{
  text-align: center;
  margin-top:20rpx;
  margin-bottom:10rpx;
}
.text{
  color:goldenrod;
  font-size:35rpx;
}
.zuji{
  color:#ccc;
  font-size:30rpx;
  margin-bottom:20rpx;
  padding-bottom:20rpx;
  border-bottom:2rpx solid #ccc;
}